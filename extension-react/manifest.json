{
  "manifest_version": 3,
  "name": "HealthyUI Chrome Extension",
  "description": "Chrome Extension for the HealthyUI app with React and Webpack",
  "version": "1.0.0",
  "permissions": [
    "webNavigation",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ]
}
